{
  "name": "@cap-js-community/websocket-test-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cds-serve --production"
  },
  "dependencies": {
    "@cap-js-community/websocket": "1.9.1",
    "@cap-js/sqlite": "2.0.4",
    "@sap/cds": "9.4.4"
  },
  "engines": {
    "node": ">=20"
  },
  "cds": {
    "features": {
      "in_memory_db": true
    },
    "requires": {
      "websocket": {
        "kind": "ws"
      },
      "auth": {
        "kind": "xsuaa"
      },
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": ":memory:"
        }
      }
    }
  }
}
